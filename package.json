{
  "name": "Clap",
  "version": "0.1.02",
  "description": "Costlocker desktop application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "css": "rimraf src/css/* && node-sass src/scss/_main.scss src/css/app.css --source-map true",
    "icons": "rimraf src/images/icons/* && /wamp64/www/clap/node_modules/.bin/electron-icon-maker --input=/wamp64/www/clap/src/images/bat_source.png --output=/wamp64/www/clap/src/images",
    "win": "/wamp64/www/clap/node_modules/.bin/electron-packager /wamp64/www/clap/ clap --overwrite --asar --platform=win32 --arch=ia32 --icon=/wamp64/www/clap/src/images/icons/win/icon.ico --prune --out=release_builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='clap'",
    "mac": "/wamp64/www/clap/node_modules/.bin/electron-packager /wamp64/www/clap/ clap --overwrite --platform=darwin --arch=x64 --icon=/wamp64/www/clap/src/images/icons/mac/icon.icns --prune --out=release_builds",
    "linux": "/wamp64/www/clap/node_modules/.bin/electron-packager /wamp64/www/clap/ clap --overwrite --platform=linux --arch=x64 --icon=/wamp64/www/clap/src/images/icons/png/1024x1024.png --prune --out=release_builds",
    "build": "echo - && echo run me in cmd.exe as admin && echo - && npm run win && npm run mac && npm run linux"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LukasMihalciny/clap.git"
  },
  "keywords": [
    "costlocker",
    "electron",
    "desktop",
    "application"
  ],
  "author": "Lukas Mihalciny",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LukasMihalciny/clap/issues"
  },
  "homepage": "https://github.com/LukasMihalciny/clap#readme",
  "devDependencies": {
    "electron": "8.2.1",
    "electron-icon-maker": "0.0.5",
    "electron-packager": "15.1.0",
    "node-sass": "4.14.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "bootstrap": "5.0.0-alpha1"
  }
}
